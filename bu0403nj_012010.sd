Program
  Name = "bu0403nj_012010";
  Type = "File";
  Domain = "SFPPRINT1";
  Padcharacter = Space;

 String L_FILE_NAME[18] = "bu0403nj_012010.cq";
 String L_SFPLOCATION_ZIP;
 String L_LOB_CODE[10];
 String L_COMPANY_ID[10];

 Unsigned Ascii Number L_PAGE_NUMBER = 1;
 Unsigned Ascii Number L_SFPMASTER_FORM[1];
 Unsigned Ascii Number L_COVERGE_POS;    -- coverage horizontal pos
 Unsigned Ascii Number L_REVISED_POS;    -- revised limit horizonal pos
 Unsigned Ascii Number L_BASIC_POS;      -- basic limit horizonal pos
 Unsigned Ascii Number L_DOLLAR_POS;
 Unsigned Ascii Number L_FACTOR_POS;
 Unsigned Ascii Number L_REVISED_FACTOR_POS;
 Unsigned Ascii Number L_BUILDING_CODE_PERCENT[3]/decimals=2;
 Unsigned Ascii Number L_LIMIT_POS;
 Unsigned Ascii Number L_LINE_PCOUNTER;
 Unsigned Ascii Number L_MAX_LINES = 50;
 Unsigned Ascii Number L_PROPERTY_LIMIT[9];
 Unsigned Ascii Number L_UTILITY_LIMIT[9];
 Unsigned Ascii Number L_MECHANICAL_LIMIT[9];
 Unsigned Ascii Number L_EXTERIOR_SIGNS_LIMIT[9];
 Unsigned Ascii Number L_VALUEABLE_PAPERS_LIMIT[9];
 Unsigned Ascii Number WATER_BACKUP_LIMIT;
 Unsigned Ascii Number L_WATER_BACKUP_LIMIT;
 Unsigned Ascii Number L_FMA100;
 Unsigned Ascii Number L_LIMIT[9];
 Unsigned Ascii Number L_BPSDEFAULT_LIMIT[9];
 Unsigned Ascii Number L_REVISED_LIMIT[9];
 Unsigned Ascii Number L_DEDUCTIBLE_POS;
 Unsigned Ascii Number L_EXTERIOR_SIGNS_LIMIT_NOT_ATTC;
 Unsigned Ascii Number L_ATTACHED_TO_BUILDING;
 Unsigned Ascii Number L_VALUABLE_PAPERS_NO;
 Unsigned Ascii Number L_DEDUCTIBLE_SIGNS_OUTDOOR;
 Unsigned Ascii Number L_WATER_DAMAGE;
 Unsigned Ascii Number L_GLASS_SQFT;
 Unsigned Ascii Number L_SIGNS_NOT_ATTACHED;
 Unsigned Ascii Number L_BU0507;
 Unsigned Ascii number L_LINE_OF_BUSINESS[4];
 Unsigned Ascii number L_STATE[2],
                       l_loop;
 wdate L_EFF_DATE;

  Include "csfonts.inc"
  Include "sfsrevname.var"
  Include "swsut100.var"
  Include "swsut200.var"

Initialization
  Deletefile (L_FILE_NAME);

Update
Access SFPNAME,
   Set SFPNAME:POLICY_NO    = SFPPRINT1:POLICY_NO,
       SFPNAME:POL_YEAR     = SFPPRINT1:POL_YEAR,
       SFPNAME:END_SEQUENCE = SFPPRINT1:END_SEQUENCE, Generic;

Access SFPPRINT4,
   Set SFPPRINT4:POLICY_NO    = SFPPRINT1:POLICY_NO,
       SFPPRINT4:POL_YEAR     = SFPPRINT1:POL_YEAR,
       SFPPRINT4:END_SEQUENCE = SFPPRINT1:END_SEQUENCE, Generic;

Access SFPLOCATION,
   Set SFPLOCATION:POLICY_NO    = SFPPRINT1:POLICY_NO,
       SFPLOCATION:POL_YEAR     = SFPPRINT1:POL_YEAR,
       SFPLOCATION:END_SEQUENCE = SFPPRINT1:END_SEQUENCE, Generic;

Access BOPGENERAL,
   Set BOPGENERAL:POLICY_NO    = SFPPRINT1:POLICY_NO,
       BOPGENERAL:POL_YEAR     = SFPPRINT1:POL_YEAR,
       BOPGENERAL:END_SEQUENCE = SFPPRINT1:END_SEQUENCE, Generic;

Access SFPMASTER,
   Set SFPMASTER:POLICY_NO    = SFPPRINT1:POLICY_NO,
       SFPMASTER:POL_YEAR     = SFPPRINT1:POL_YEAR,
       SFPMASTER:END_SEQUENCE = SFPPRINT1:END_SEQUENCE, Exact;

L_SFPMASTER_FORM = SFPMASTER:FORM;

Access BPSDEFAULT,
   Set BPSDEFAULT:COMPANY_ID       = SFPNAME:COMPANY_ID,
       BPSDEFAULT:LINE_OF_BUSINESS = SFPNAME:LINE_OF_BUSINESS,
       BPSDEFAULT:FORM             = L_SFPMASTER_FORM, Generic

Access BPSDEFAULT2,
   Set bpsdefault2:COMPANY_ID         = SFPNAME:COMPANY_ID,
       bpsdefault2:STATE              = I_STATE,
       bpsdefault2:LINE_OF_BUSINESS   = SFPNAME:LINE_OF_BUSINESS,
       bpsdefault2:SUPPLEMENTAL_FORM  = L_SFPMASTER_FORM, Generic

Access SFSLINE,
   Set SFSLINE:COMPANY_ID       = SFPNAME:COMPANY_ID,
       SFSLINE:LINE_OF_BUSINESS = SFPNAME:LINE_OF_BUSINESS, Generic;

L_COMPANY_ID = SFPNAME:COMPANY_ID
L_LOB_CODE   = SFSLINE:LOB_CODE

Include "swsut100.inc"
I_CODE = "BU0403"

Access SFSSTATE,
   Set SFSSTATE:COMPANY_ID = L_COMPANY_ID,
       SFSSTATE:STATE      = I_STATE,
       SFSSTATE:COUNTY     = "000", Exact

Access SCIPSCONTROL,
   Set SCIPSCONTROL:COMPANY_ID   = L_COMPANY_ID,
       SCIPSCONTROL:STATE        = I_STATE,
       SCIPSCONTROL:LOB_CODE     = L_LOB_CODE, Exact

If SCIPSCONTROL:DUPLEX_SHORT_EDGE = 1 Then
  {
    Writefile(L_FILE_NAME,"%s",L_SEDUP)
  }
Else
  {
    Writefile(L_FILE_NAME,"%s",L_LEDUP)
  }

--****SCIPSCONTROL VARIABLES****--

L_ATTACHED_TO_BUILDING     = SCIPSCONTROL:SIGNS_ATTACHED_TO_BUILDING
L_DEDUCTIBLE_SIGNS_OUTDOOR = SCIPSCONTROL:SIGNS_ATTACHED_DEDUCTIBLE
L_VALUABLE_PAPERS_NO       = SCIPSCONTROL:VALUABLE_PAPERS_PAGE_NO
L_WATER_DAMAGE             = SCIPSCONTROL:WATER_DAMAGE
L_GLASS_SQFT               = SCIPSCONTROL:GLASS_SQUARE_FEET
L_SIGNS_NOT_ATTACHED       = SIGNS_NOT_ATTACHED_TO_BUILDING

--****SCIPSCONTROL VARIABLES****--

While SFPLOCATION:POLICY_NO    = SFPPRINT1:POLICY_NO And
      SFPLOCATION:POL_YEAR     = SFPPRINT1:POL_YEAR And
      SFPLOCATION:END_SEQUENCE = SFPPRINT1:END_SEQUENCE
  {
    L_SFPLOCATION_ZIP = If (Val(SFPLOCATION:STR_ZIPCODE[6,9]) = 0) Then
                          SFPLOCATION:STR_ZIPCODE[1,5]
                        Else
                          SFPLOCATION:STR_ZIPCODE[1,5] + "-" +
                          SFPLOCATION:STR_ZIPCODE[6,9]
--border

If SCIPSCONTROL:PRINT_BORDERS_ON_DEC_PAGE = 1 Then
    {
      Writefile(L_FILE_NAME,"%s","<033>&a140h-220V<033>*c10a3100b0P") --left vertical li
      Writefile(L_FILE_NAME,"%s","<033>&a5585h-0V<033>*c10a3100b0P")  --right vertical
      Writefile(L_FILE_NAME,"%s","<033>&a140h-0V<033>*c5467h20v0P")   --top horizontal line to
      Writefile(L_FILE_NAME,"%s","<033>&a140h7300V<033>*c5467h20v0P") --bottom horizontal line
    }

L_PAGE_NUMBER      = 1
L_STATE            = BOPGENERAL:STATE
L_COMPANY_ID       = sfpname:COMPANY_ID
L_LINE_OF_BUSINESS = sfpname:LINE_OF_BUSINESS

If sfpname:company_id = "FLEMINGTON" then
  {
   Access sfsend,
      Set sfsend:COMPANY_ID       = L_COMPANY_ID,
          sfsend:STATE            = L_STATE,
          sfsend:LINE_OF_BUSINESS = L_LINE_OF_BUSINESS,
          sfsend:form             = sfpmaster:form,
          sfsend:CODE             = I_CODE, generic

    While sfsend:COMPANY_ID       = L_COMPANY_ID and
          sfsend:STATE            = L_STATE and
          sfsend:LINE_OF_BUSINESS = l_LINE_OF_BUSINESS and
          sfsend:form             = sfpmaster:form and
          sfsend:CODE             = I_CODE
      {
        --start of sfsend check
        If (sfpname:eff_date >= sfsend:eff_date and
           (sfpname:eff_date < sfsend:exp_date or
            sfsend:exp_date = 0)) then
          {
            l_eff_date = sfsend:eff_date
          }
        next sfsend
      }

    Access sfsend,
       Set sfsend:COMPANY_ID       = L_COMPANY_ID,
           sfsend:STATE            = L_STATE,
           sfsend:LINE_OF_BUSINESS = L_LINE_OF_BUSINESS,
           sfsend:form             = sfpmaster:form,
           sfsend:CODE             = I_CODE,
           sfsend:EFF_DATE         = L_EFF_DATE, exact

    Writefile(L_FILE_NAME,"%s",L_ARIAL_9)
    Writefile(L_FILE_NAME,"%s","<033>&a420h7200V")
    Writefile(L_FILE_NAME,"%s",l_copyright_6)
    Writefile(L_FILE_NAME,"%s"," ")
    Writefile(L_FILE_NAME,"%s",L_ARIAL_9)
    Writefile(L_FILE_NAME,"%s","2010 MSO, Inc.")
    Writefile(L_FILE_NAME,"%s","<033>&a200h+120V")

    Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD_12)
    Writefile(L_FILE_NAME,"%s","<033>&a4700h0V")
    Writefile(L_FILE_NAME,"%s",Trun(SFSEND:FORM_PRINT))
    Writefile(L_FILE_NAME,"%s"," ")
    Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD_9)
    Writefile(L_FILE_NAME,"%s",SFSEND:EDITION_PRINT)
  }
else
  {
   Access SFSOPTEND,
      Set SFSOPTEND:COMPANY_ID       = L_COMPANY_ID,
          SFSOPTEND:STATE            = L_STATE,
          SFSOPTEND:LINE_OF_BUSINESS = L_LINE_OF_BUSINESS,
          SFSOPTEND:CODE             = I_CODE, generic

    While SFSOPTEND:COMPANY_ID       = L_COMPANY_ID and
          SFSOPTEND:STATE            = L_STATE and
          SFSOPTEND:LINE_OF_BUSINESS = l_LINE_OF_BUSINESS and
          SFSOPTEND:CODE             = I_CODE
      {
        --start of sfsoptend check
        If (sfpname:eff_date >= sfsoptend:eff_date and
           (sfpname:eff_date < sfsoptend:exp_date or
            sfsoptend:exp_date = 0)) then
          {
            l_eff_date = sfsoptend:eff_date
          }
        next sfsoptend
      }

    Access SFSOPTEND,
       Set SFSOPTEND:COMPANY_ID       = L_COMPANY_ID,
           SFSOPTEND:STATE            = L_STATE,
           SFSOPTEND:LINE_OF_BUSINESS = L_LINE_OF_BUSINESS,
           SFSOPTEND:CODE             = I_CODE,
           SFSOPTEND:EFF_DATE         = L_EFF_DATE, exact

    If sfsoptend:edition <> 0 then
      {
        Writefile(L_FILE_NAME,"%s",L_ARIAL_9)
        Writefile(L_FILE_NAME,"%s","<033>&a420h7080V")
        Writefile(L_FILE_NAME,"%s","Ed. ")
        Writefile(L_FILE_NAME,"%n",sfsoptend:edition)
      }
    Writefile(L_FILE_NAME,"%s",L_ARIAL_9)
    Writefile(L_FILE_NAME,"%s","<033>&a420h7200V")
    Writefile(L_FILE_NAME,"%s",l_copyright_6)
    Writefile(L_FILE_NAME,"%s"," ")
    Writefile(L_FILE_NAME,"%s",L_ARIAL_9)
    Writefile(L_FILE_NAME,"%s",sfsoptend:COPYRIGHT)
    Writefile(L_FILE_NAME,"%s","<033>&a200h+120V")

    Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD_12)
    Writefile(L_FILE_NAME,"%s","<033>&a4700h0V")
    Writefile(L_FILE_NAME,"%s",Trun(SFSOPTEND:FORM_PRINT))
    Writefile(L_FILE_NAME,"%s"," ")
    Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD_9)
    Writefile(L_FILE_NAME,"%s",SFSOPTEND:EDITION_PRINT)
  }

  Writefile(L_FILE_NAME,"%s","<033>&a200h+120V")

  Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD_12)
  Writefile(L_FILE_NAME,"%s","<033>&a180h0V")
  Writefile(L_FILE_NAME,"%s","DECLARATIONS SUPPLEMENT - ")
  Writefile(L_FILE_NAME,"%s",SFSSTATE:DESCRIPTION)
  Writefile(L_FILE_NAME,"%s","<033>&a140h+60V<033>*c5467h5v0P")
  Writefile(L_FILE_NAME,"%s","<NL>")

  Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD_9)
  Writefile(L_FILE_NAME,"%s","<033>&a180h+30V")
  Writefile(L_FILE_NAME,"%s","Policy No: ")

  Writefile(L_FILE_NAME,"%s",Trun(SFSLINE:ALPHA))

  Writefile(L_FILE_NAME,"%n",SFPPRINT1:POLICY_NO)
  Writefile(L_FILE_NAME,"%s","<033>&a2250h+0V")
  Writefile(L_FILE_NAME,"%s","Effective: ")
  Writefile(L_FILE_NAME,"%d",SFPNAME:EFF_DATE)
  Writefile(L_FILE_NAME,"%s","<NL>")

  Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD_9)
  Writefile(L_FILE_NAME,"%s","<033>&a400h+20V")
  Writefile(L_FILE_NAME,"%s","Each basic limit ")
  Writefile(L_FILE_NAME,"%s","<033>&a1110h-30V<033>*c90h2v0P")
  Writefile(L_FILE_NAME,"%s","<033>&a1200h+30V")
  Writefile(L_FILE_NAME,"%s"," or if increased, each revised limit ")
  Writefile(L_FILE_NAME,"%s","<033>&a2680h-30V<033>*c90h2v0P")
  Writefile(L_FILE_NAME,"%s","<033>&a2790h+30V")
  Writefile(L_FILE_NAME,"%s","shown below is")
  Writefile(L_FILE_NAME,"%s",L_ARIAL_ITALIC_BOLD_9)
  Writefile(L_FILE_NAME,"%s", " our ")
  Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD_9)
  Writefile(L_FILE_NAME,"%s","maximum limit of liability for the coverage ")
  Writefile(L_FILE_NAME,"%s","<NL>")

  Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD_9)
  Writefile(L_FILE_NAME,"%s","<033>&a400h+0V")
  Writefile(L_FILE_NAME,"%s","in any occurrence, except as otherwise stated.")
  Writefile(L_FILE_NAME,"%s","<NL>")
--  Writefile(L_FILE_NAME,"%s","<NL>")

  Writefile(L_FILE_NAME,"%s","<NL>")

  Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
  Writefile(L_FILE_NAME,"%s","<033>&a180h+0V")
  Writefile(L_FILE_NAME,"%s","Location")
  Writefile(L_FILE_NAME,"%s","<033>&a900h+0V")
  Writefile(L_FILE_NAME,"%n",SFPLOCATION:PREM_NO)
  Writefile(L_FILE_NAME,"%s","<033>&a1200h+0V")
  Writefile(L_FILE_NAME,"%s","Building")
  Writefile(L_FILE_NAME,"%s","<033>&a1900h+0V")
  Writefile(L_FILE_NAME,"%n",SFPLOCATION:BUILD_NO)

  Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
  Writefile(L_FILE_NAME,"%s","<033>&a2500h+0V")
  Writefile(L_FILE_NAME,"%s",SFPLOCATION:ADDRESS)
  Writefile(L_FILE_NAME,"%s","<NL>")
  For l_loop = 1 to 3
    {
      If SFPLOCATION:ADDRESS1[l_loop] <> "" Then
        {
          Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
          Writefile(L_FILE_NAME,"%s","<033>&a2500h+0V")
          Writefile(L_FILE_NAME,"%s",SFPLOCATION:ADDRESS1[l_loop])
          Writefile(L_FILE_NAME,"%s","<NL>")
        }
    }

  Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
  Writefile(L_FILE_NAME,"%s","<033>&a2500h+0V")
  Writefile(L_FILE_NAME,"%s",Trun(SFPLOCATION:CITY))
  Writefile(L_FILE_NAME,"%s",", ")
  Writefile(L_FILE_NAME,"%s",SFPLOCATION:STR_STATE)
  Writefile(L_FILE_NAME,"%s"," ")
  Writefile(L_FILE_NAME,"%s",L_SFPLOCATION_ZIP)
  Writefile(L_FILE_NAME,"%s","<NL>")
  Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
  Writefile(L_FILE_NAME,"%s","<033>&a1570h+40V")
  Writefile(L_FILE_NAME,"%s","COVERAGE")
  Writefile(L_FILE_NAME,"%s","<NL>")
  Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
  Writefile(L_FILE_NAME,"%s","<033>&a1690h+0V")
  Writefile(L_FILE_NAME,"%s","PART I")
  Writefile(L_FILE_NAME,"%s","<nl>")

  L_COVERGE_POS = 180
  L_BASIC_POS = 2550
  L_REVISED_POS = 2430
  L_DOLLAR_POS = 3575
  L_DEDUCTIBLE_POS = 1840
  L_FACTOR_POS = 2745
  L_REVISED_FACTOR_POS = 2350
  L_LIMIT_POS = 3650

  Access bpsdefault2,
     Set bpsdefault2:COMPANY_ID       = L_COMPANY_ID,
         bpsdefault2:STATE            = bopgeneral:state,
         bpsdefault2:LINE_OF_BUSINESS = bopgeneral:line_of_business,
         bpsdefault2:supplemental_form  = I_CODE, generic

   While bpsdefault2:COMPANY_ID       = L_COMPANY_ID and
         bpsdefault2:STATE            = bopgeneral:STATE and
         bpsdefault2:LINE_OF_BUSINESS = bopgeneral:LINE_OF_BUSINESS and
         bpsdefault2:supplemental_form  = I_CODE
     {
       --start of bpsdefault2 check
       If (sfpname:eff_date >= bpsdefault2:eff_date and
          (sfpname:eff_date < bpsdefault2:exp_date or
           bpsdefault2:exp_date = 0)) then
         {
           l_eff_date = bpsdefault2:eff_date
         }
       next bpsdefault2
     }

   Access bpsdefault2,
      Set bpsdefault2:COMPANY_ID       = L_COMPANY_ID,
          bpsdefault2:STATE            = bopgeneral:STATE,
          bpsdefault2:LINE_OF_BUSINESS = bopgeneral:LINE_OF_BUSINESS,
          bpsdefault2:supplemental_form = I_CODE,
          bpsdefault2:EFF_DATE         = l_eff_date, exact

  Access BOPGENERAL ,
     Set BOPGENERAL:POLICY_NO    = SFPPRINT1:POLICY_NO,
         BOPGENERAL:POL_YEAR     = SFPPRINT1:POL_YEAR,
         BOPGENERAL:END_SEQUENCE = SFPPRINT1:END_SEQUENCE,
         BOPGENERAL:PREM_NO      = SFPLOCATION:PREM_NO,
         BOPGENERAL:BUILD_NO     = SFPLOCATION:BUILD_NO,Generic

  Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
  Writefile(L_FILE_NAME,"%s","<033>&a180h+60V")
  Writefile(L_FILE_NAME,"%s","Accounts Receivable ")
  Writefile(L_FILE_NAME,"%s",L_ARIAL_8)
  Writefile(L_FILE_NAME,"%s","<033>&a-30V")
  Writefile(L_FILE_NAME,"%s","(1)")
  Writefile(L_FILE_NAME,"%s","<033>&a+30V")
  Writefile(L_FILE_NAME,"%s",L_ARIAL)
  Writefile(L_FILE_NAME,"%s","<033>&a")
  Writefile(L_FILE_NAME,"%n",L_BASIC_POS)
  Writefile(L_FILE_NAME,"%s","h+0V")
  Writefile(L_FILE_NAME,"%s","Basic Limit:")
  Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
  Writefile(L_FILE_NAME,"%s","<033>&a")
  Writefile(L_FILE_NAME,"%n",L_DOLLAR_POS)
  Writefile(L_FILE_NAME,"%s","h+0V")
  Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
  Writefile(L_FILE_NAME,"%s","$")                  --Basic Limit Accounts Receivable

  L_LIMIT            = BOPGENERAL:PROPERTY_LIMIT * (bpsdefault2:AR_PERCENT Divide 100)
  L_REVISED_LIMIT    = BOPGENERAL:AR_LIMIT
  L_BPSDEFAULT_LIMIT = bpsdefault2:AR_LIMIT

  Do PRINT_LIMIT

  Writefile(L_FILE_NAME,"%s%s%n%s%s",L_ARIAL_BOLD,"<033>&a",
                                  L_COVERGE_POS,"h+60V","Building")

  Writefile(L_FILE_NAME,"%s%s%s%n%s%s%s%s","<NL>",L_ARIAL_BOLD,"<033>&a",
                                  L_COVERGE_POS,"h+0V","E. ",l_arial,"Supplies and Materials: Loss by Theft")

  Writefile(L_FILE_NAME,"%s%s%n%s%s%s%n%s%s%s",L_ARIAL,"<033>&a",
                                    L_BASIC_POS+200,"h+0V","Limit:",
                                    "<033>&a",L_DOLLAR_POS,"h+0V",L_ARIAL_BOLD,"$")

  If BPSdefault2:SUPPLIES_LIMIT > 0 Then
    {
       Writefile(L_FILE_NAME,"%s%s%n%s%s%s",L_LETTER_GOTHIC_BOLD_12,"<033>&a",
                                  L_LIMIT_POS,"h+0V",
                                  Str(BPSdefault2:SUPPLIES_LIMIT,"ZZZ,ZZZ,ZZ9"),
                                  "<NL>")
    }


  Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
  Writefile(L_FILE_NAME,"%s","<033>&a")
  Writefile(L_FILE_NAME,"%n",L_COVERGE_POS)
  Writefile(L_FILE_NAME,"%s","h+60V")
  Writefile(L_FILE_NAME,"%s","Building Code / Law Coverage*")

  Writefile(L_FILE_NAME,"%s","<033>&a")
  Writefile(L_FILE_NAME,"%n",L_FACTOR_POS)
  Writefile(L_FILE_NAME,"%s","h+0V")
  Writefile(L_FILE_NAME,"%s",L_ARIAL)
  Writefile(L_FILE_NAME,"%s","Factor:")                  --factor

  L_BUILDING_CODE_PERCENT = bpsdefault2:BUILDING_CODE_factor Divide 100
  If SCIPSCONTROL:PRINT_REVISED_ON_SUPP = 1 Then
    {
      If bpsdefault2:BUILDING_CODE_factor >= BOPGENERAL:BUILDING_CODE_PERCENT Then
        {
          Writefile(L_FILE_NAME,"%s","<033>&a")
          Writefile(L_FILE_NAME,"%n",L_LIMIT_POS)
          Writefile(L_FILE_NAME,"%s","h+0V")
          Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
          Writefile(L_FILE_NAME,"%s",Str(L_BUILDING_CODE_PERCENT,"ZZZZ,ZZZ.99"))
          Writefile(L_FILE_NAME,"%s","<NL>")
        }
      Writefile(L_FILE_NAME,"%s","<033>&a")
      Writefile(L_FILE_NAME,"%n",L_REVISED_FACTOR_POS)
      Writefile(L_FILE_NAME,"%s","h+0V")
      Writefile(L_FILE_NAME,"%s",L_ARIAL)
      Writefile(L_FILE_NAME,"%s","Revised Factor:")                  --revised factor
      If (bpsdefault2:BUILDING_CODE_factor <> BOPGENERAL:BUILDING_CODE_PERCENT) Then
        {
          Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
          Writefile(L_FILE_NAME,"%s","<033>&a")
          Writefile(L_FILE_NAME,"%n",L_LIMIT_POS)
          Writefile(L_FILE_NAME,"%s","h+0V")
          Writefile(L_FILE_NAME,"%s",Str(BOPGENERAL:BUILDING_CODE_PERCENT,"ZZZ,ZZZ.99"))

        }
    }  --scips control if
  Else
    {
      If (bpsdefault2:BUILDING_CODE_factor >= BOPGENERAL:BUILDING_CODE_PERCENT) Then
        {
          Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
          Writefile(L_FILE_NAME,"%s","<033>&a")
          Writefile(L_FILE_NAME,"%n",L_LIMIT_POS)
          Writefile(L_FILE_NAME,"%s","h+0V")
          Writefile(L_FILE_NAME,"%s",Str(L_BUILDING_CODE_PERCENT,"ZZZZ,ZZZ.99"))
          Writefile(L_FILE_NAME,"%s","<NL>")
        }
      Else
        {
          Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
          Writefile(L_FILE_NAME,"%s","<033>&a")
          Writefile(L_FILE_NAME,"%n",L_LIMIT_POS)
