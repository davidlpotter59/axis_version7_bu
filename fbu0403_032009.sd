                          L_DOLLAR_POS,"h+0V",L_ARIAL_BOLD,"$")

    Writefile(L_FILE_NAME,"%s%s%n%s%s",L_LETTER_GOTHIC_BOLD_12,"<033>&a",
                          L_LIMIT_POS,"h+0V",
                          Str(BOPGENERAL:GLASS_LIMIT,"ZZZ,ZZZ,ZZ9"))

    Writefile(L_FILE_NAME,"%s%s%n%s%s",L_ARIAL,"<033>&a",
