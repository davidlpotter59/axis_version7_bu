    Writefile(L_FILE_NAME,"%s%n%s%s%s","<033>&a",
                           L_DOLLAR_POS,"h+0V",L_ARIAL_BOLD,"$")

    Writefile(L_FILE_NAME,"%s%s%n%s%s",L_LETTER_GOTHIC_BOLD_12,"<033>&a",
                          L_LIMIT_POS,"h+0V",
                          Str(BOPGENERAL:GLASS_DEDUCTIBLE,"ZZZ,ZZZ,ZZ9"))

    Writefile(L_FILE_NAME,"%s",L_ARIAL_9)
    Writefile(L_FILE_NAME,"%s","<033>&a5200h7200V")
    Writefile(L_FILE_NAME,"%s","Page ")
    Writefile(L_FILE_NAME,"%n",l_page_number + 1)
  }
End --End PRINT_PAGE_3


