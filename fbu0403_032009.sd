Program
  Name = "fbu0403_032009";
  Type = "File";
  Domain = "SFPPRINT1";
  Padcharacter = Space;

 String L_FILE_NAME[17] = "fbu0403_032009.cq";
 String L_SFPLOCATION_ZIP,
        l_enter[1];
 String L_LOB_CODE[10];
 String L_COMPANY_ID[10];
 string l_font[35] ;

 unsigned ascii number l_ar_limit[8] ;
 unsigned ascii number l_building_code_percent1[2] ;
 unsigned ascii number l_building_code_percent2[2] ;
 unsigned ascii number l_customer_prop_limit[8] ;
 unsigned ascii number l_utility_limit1[8] ;
 unsigned ascii number l_mechanical_limit1[8] ;
 unsigned ascii number l_off_premises_b[8] ;
 unsigned ascii number l_signs_limit[8] ;
 unsigned ascii number l_valuable_papers_limit1[8] ;
 unsigned ascii number l_building_glass[8] ;
 unsigned ascii number l_fire_limit[8],
                       l_pollution_limit[9] ;
 Unsigned Ascii Number L_PAGE_NUMBER = 0;
 Unsigned Ascii Number L_SFPMASTER_FORM[1];
 Unsigned Ascii Number L_COVERGE_POS;    -- coverage horizontal pos
 Unsigned Ascii Number L_REVISED_POS;    -- revised limit horizonal pos
 Unsigned Ascii Number L_BASIC_POS;      -- basic limit horizonal pos
 Unsigned Ascii Number L_DOLLAR_POS;
 Unsigned Ascii Number L_FACTOR_POS;
 Unsigned Ascii Number L_REVISED_FACTOR_POS;
 Unsigned Ascii Number L_BUILDING_CODE_PERCENT[3]/decimals=2;
 Unsigned Ascii Number L_LIMIT_POS;
 Unsigned Ascii Number L_LINE_PCOUNTER;
 Unsigned Ascii Number L_MAX_LINES = 50;
 Unsigned Ascii Number L_PROPERTY_LIMIT[9];
 Unsigned Ascii Number L_UTILITY_LIMIT[9];
 Unsigned Ascii Number L_MECHANICAL_LIMIT[9];
 Unsigned Ascii Number L_EXTERIOR_SIGNS_LIMIT[9];
 Unsigned Ascii Number L_VALUEABLE_PAPERS_LIMIT[9];
 Unsigned Ascii Number WATER_BACKUP_LIMIT;
 Unsigned Ascii Number L_WATER_BACKUP_LIMIT;
 Unsigned Ascii Number L_FMA100;
 Unsigned Ascii Number L_LIMIT[9];
 Unsigned Ascii Number L_BPSDEFAULT_LIMIT[9];
 Unsigned Ascii Number L_BPSSUPPLEMENTAL_LIMIT[9];
 Unsigned Ascii Number L_REVISED_LIMIT[9];
 Unsigned Ascii Number L_DEDUCTIBLE_POS;
 Unsigned Ascii Number L_EXTERIOR_SIGNS_LIMIT_NOT_ATTC;
 Unsigned Ascii Number L_ATTACHED_TO_BUILDING;
 Unsigned Ascii Number L_VALUABLE_PAPERS_NO;
 Unsigned Ascii Number L_DEDUCTIBLE_SIGNS_OUTDOOR;
 Unsigned Ascii Number L_WATER_DAMAGE;
 Unsigned Ascii Number L_GLASS_SQFT;
 Unsigned Ascii Number L_SIGNS_NOT_ATTACHED;
 Unsigned Ascii Number L_BU0507;
 Unsigned Ascii number L_LINE_OF_BUSINESS[4];
 Unsigned Ascii number L_STATE[2];
 Unsigned Ascii number L_DO_ONCE,
                       l_computer_coverage_hardware[9],
                       l_computer_coverage_media[9],
                       l_employee_dishonesty[9],
                       L_BOPGENERAL_BUILDING_CODE_PERCENT;
 wdate L_EFF_DATE;
 wdate L_EFF_DATE_1;

  Include "csfonts.inc"
  Include "sfsrevname.var"
  Include "swsut100.var"
  Include "swsut200.var"
  Include "header.var"


Initialization
  Deletefile (L_FILE_NAME);

Update
Access SFPNAME,
   Set SFPNAME:POLICY_NO    = SFPPRINT1:POLICY_NO,
       SFPNAME:POL_YEAR     = SFPPRINT1:POL_YEAR,
       SFPNAME:END_SEQUENCE = SFPPRINT1:END_SEQUENCE, Generic;

access sfpend,
   set sfpend:policy_no    = sfpprint1:policy_no,
       sfpend:pol_year     = sfpprint1:pol_year,
       sfpend:end_sequence = sfpprint1:end_sequence, generic

Access SFPPRINT4,
   Set SFPPRINT4:POLICY_NO    = SFPPRINT1:POLICY_NO,
       SFPPRINT4:POL_YEAR     = SFPPRINT1:POL_YEAR,
       SFPPRINT4:END_SEQUENCE = SFPPRINT1:END_SEQUENCE, Generic;

Access SFPLOCATION,
   Set SFPLOCATION:POLICY_NO    = SFPPRINT1:POLICY_NO,
       SFPLOCATION:POL_YEAR     = SFPPRINT1:POL_YEAR,
       SFPLOCATION:END_SEQUENCE = SFPPRINT1:END_SEQUENCE, Generic;

Access BOPGENERAL,
   Set BOPGENERAL:POLICY_NO    = SFPPRINT1:POLICY_NO,
       BOPGENERAL:POL_YEAR     = SFPPRINT1:POL_YEAR,
       BOPGENERAL:END_SEQUENCE = SFPPRINT1:END_SEQUENCE, Generic;

Access SFPMASTER,
   Set SFPMASTER:POLICY_NO    = SFPPRINT1:POLICY_NO,
       SFPMASTER:POL_YEAR     = SFPPRINT1:POL_YEAR,
       SFPMASTER:END_SEQUENCE = SFPPRINT1:END_SEQUENCE, Exact;

L_SFPMASTER_FORM = SFPMASTER:FORM;

Access BPSDEFAULT,
   Set BPSDEFAULT:COMPANY_ID       = SFPNAME:COMPANY_ID,
       BPSDEFAULT:LINE_OF_BUSINESS = SFPNAME:LINE_OF_BUSINESS,
       BPSDEFAULT:FORM             = L_SFPMASTER_FORM, Generic

Access SFSLINE,
   Set SFSLINE:COMPANY_ID       = SFPNAME:COMPANY_ID,
       SFSLINE:LINE_OF_BUSINESS = SFPNAME:LINE_OF_BUSINESS,
       sfsline:lob_subline      = "00",exact

Access SFSCOMPANY,
   Set SFSCOMPANY:COMPANY_ID = SFPNAME:COMPANY_ID, Exact

L_COMPANY_ID = SFPNAME:COMPANY_ID
L_LINE_OF_BUSINESS = sfpname:LINE_OF_BUSINESS
L_LOB_CODE   = SFSLINE:LOB_CODE

Include "swsut100.inc"
I_CODE = "FBU0403"

Access SFSSTATE,
   Set SFSSTATE:COMPANY_ID = L_COMPANY_ID,
       SFSSTATE:STATE      = I_STATE,
       SFSSTATE:COUNTY     = "000", Exact

Access SCIPSCONTROL,
   Set SCIPSCONTROL:COMPANY_ID   = L_COMPANY_ID,
       SCIPSCONTROL:STATE        = I_STATE,
       SCIPSCONTROL:LOB_CODE     = L_LOB_CODE, exact

If SCIPSCONTROL:DUPLEX_SHORT_EDGE = 1 Then
  {
    Writefile(L_FILE_NAME,"%s",L_SEDUP)
  }
Else
  {
    Writefile(L_FILE_NAME,"%s",L_LEDUP)
  }

--****SCIPSCONTROL VARIABLES****--

L_ATTACHED_TO_BUILDING     = SCIPSCONTROL:SIGNS_ATTACHED_TO_BUILDING
L_DEDUCTIBLE_SIGNS_OUTDOOR = SCIPSCONTROL:SIGNS_ATTACHED_DEDUCTIBLE
L_VALUABLE_PAPERS_NO       = SCIPSCONTROL:VALUABLE_PAPERS_PAGE_NO
L_WATER_DAMAGE             = SCIPSCONTROL:WATER_DAMAGE
L_GLASS_SQFT               = SCIPSCONTROL:GLASS_SQUARE_FEET
L_SIGNS_NOT_ATTACHED       = SIGNS_NOT_ATTACHED_TO_BUILDING
L_DO_ONCE                  = 0


While sfpend:POLICY_NO    = SFPPRINT1:POLICY_NO    And
      sfpend:POL_YEAR     = SFPPRINT1:POL_YEAR     And
      sfpend:END_SEQUENCE = SFPPRINT1:END_SEQUENCE --and
      --sfpend:prem_no      = bopgeneral:prem_no     and
      --sfpend:build_no     = bopgeneral:build_no
  {
    If SFPEND:CODE = I_CODE Then
      {
      /*
        Access SFPLOCATION,
           Set SFPLOCATION:POLICY_NO    = SFPPRINT1:POLICY_NO ,
               SFPLOCATION:POL_YEAR     = SFPPRINT1:POL_YEAR ,
               SFPLOCATION:END_SEQUENCE = SFPPRINT1:END_SEQUENCE,
               SFPLOCATION:PREM_NO      = SFPEND:PREM_NO,
               SFPLOCATION:BUILD_NO     = SFPEND:BUILD_NO, Generic

        Access BOPENDORSE,
           Set BOPENDORSE:POLICY_NO    = SFPPRINT1:POLICY_NO ,
               BOPENDORSE:POL_YEAR     = SFPPRINT1:POL_YEAR ,
               BOPENDORSE:END_SEQUENCE = SFPPRINT1:END_SEQUENCE,
               BOPENDORSE:PREM_NO      = SFPEND:PREM_NO,
               BOPENDORSE:BUILD_NO     = SFPEND:BUILD_NO,
               BOPENDORSE:CODE         = I_CODE, Generic
     */
        L_SFPLOCATION_ZIP = If (Val(SFPLOCATION:STR_ZIPCODE[6,9]) = 0) Then
                                SFPLOCATION:STR_ZIPCODE[1,5]
                            Else
                                SFPLOCATION:STR_ZIPCODE[1,5] + "-" +
                                SFPLOCATION:STR_ZIPCODE[6,9]

        L_STATE = BOPGENERAL:STATE

        Access BPSSUPPLEMENTAL,
           Set BPSSUPPLEMENTAL:COMPANY_ID       = L_COMPANY_ID,
               BPSSUPPLEMENTAL:STATE            = L_STATE,
               BPSSUPPLEMENTAL:LINE_OF_BUSINESS = L_LINE_OF_BUSINESS, generic

         While BPSSUPPLEMENTAL:COMPANY_ID       = L_COMPANY_ID and
               BPSSUPPLEMENTAL:STATE            = L_STATE and
               BPSSUPPLEMENTAL:LINE_OF_BUSINESS = L_LINE_OF_BUSINESS
           {
             if BPSSUPPLEMENTAL:eff_date <= sfpname:eff_date then
               {
                 l_eff_date = BPSSUPPLEMENTAL:eff_date
                 l_ar_limit                   = bpssupplemental:ar_limit
                 l_building_code_percent1     = bpssupplemental:building_code_percent
                 l_customer_prop_limit        = bpssupplemental:customer_prop_limit
                 l_utility_limit1             = bpssupplemental:utility_limit
                 l_mechanical_limit1          = bpssupplemental:mechanical_limit
                 l_off_premises_b             = bpssupplemental:off_premises_b
                 l_signs_limit                = bpssupplemental:signs_limit
                 l_valuable_papers_limit1     = bpssupplemental:valuable_papers_limit
                 l_building_glass             = bpssupplemental:building_glass
                 l_fire_limit                 = bpssupplemental:fire_limit
                 l_pollution_limit            = bpssupplemental:pollution_limit
                 l_water_backup_limit         = bpssupplemental:water_backup_limit
                 l_computer_coverage_hardware = bpssupplemental:COMPUTER_COVERAGE_HARDWARE
                 l_COMPUTER_COVERAGE_MEDIA    = bpssupplemental:COMPUTER_COVERAGE_MEDIA
                 l_employee_dishonesty        = bpssupplemental:employee_dishonesty
               }
             Next BPSSUPPLEMENTAL
           }
        Access BPSSUPPLEMENTAL,
           Set BPSSUPPLEMENTAL:COMPANY_ID       = SFPNAME:COMPANY_ID,
               BPSSUPPLEMENTAL:STATE            = L_STATE,
               BPSSUPPLEMENTAL:LINE_OF_BUSINESS = SFPNAME:LINE_OF_BUSINESS,
               BPSSUPPLEMENTAL:eff_date         = l_eff_date, exact

        Access SFSOPTEND,
           Set SFSOPTEND:COMPANY_ID       = L_COMPANY_ID,
               SFSOPTEND:STATE            = L_STATE,
               SFSOPTEND:LINE_OF_BUSINESS = L_LINE_OF_BUSINESS,
               SFSOPTEND:CODE             = I_CODE, generic

         While SFSOPTEND:COMPANY_ID       = L_COMPANY_ID and
               SFSOPTEND:STATE            = L_STATE and
               SFSOPTEND:LINE_OF_BUSINESS = l_LINE_OF_BUSINESS and
               SFSOPTEND:CODE             = I_CODE
           {
             --start of sfsoptend check
             If (sfpname:eff_date >= sfsoptend:eff_date and
                (sfpname:eff_date < sfsoptend:exp_date or
                 sfsoptend:exp_date = 0)) then
               {
                 l_eff_date_1 = sfsoptend:eff_date
               }
             next sfsoptend
           }

        Access SFSOPTEND,
           Set SFSOPTEND:COMPANY_ID       = L_COMPANY_ID,
               SFSOPTEND:STATE            = L_STATE,
               SFSOPTEND:LINE_OF_BUSINESS = L_LINE_OF_BUSINESS,
               SFSOPTEND:CODE             = I_CODE,
               SFSOPTEND:EFF_DATE         = L_EFF_DATE_1, exact


        --If SFPEND:CODE = I_CODE Then
        --  {
            Access SFPLOCATION,
               Set SFPLOCATION:POLICY_NO    = SFPPRINT1:POLICY_NO ,
                   SFPLOCATION:POL_YEAR     = SFPPRINT1:POL_YEAR ,
                   SFPLOCATION:END_SEQUENCE = SFPPRINT1:END_SEQUENCE,
                   SFPLOCATION:PREM_NO      = SFPEND:PREM_NO,
                   SFPLOCATION:BUILD_NO     = SFPEND:BUILD_NO, Generic

            Access BOPENDORSE,
               Set BOPENDORSE:POLICY_NO    = SFPPRINT1:POLICY_NO ,
                   BOPENDORSE:POL_YEAR     = SFPPRINT1:POL_YEAR ,
                   BOPENDORSE:END_SEQUENCE = SFPPRINT1:END_SEQUENCE,generic

            Writefile(L_FILE_NAME,"%s","<033>&a200h+120V")

            If sfsoptend:edition <> 0 then
              {
                Writefile(L_FILE_NAME,"%s",L_ARIAL_9)
                Writefile(L_FILE_NAME,"%s","<033>&a420h7080V")
                Writefile(L_FILE_NAME,"%s","Ed. ")
                Writefile(L_FILE_NAME,"%n",sfsoptend:edition)
              }

            Writefile(L_FILE_NAME,"%s",L_ARIAL_9)
            Writefile(L_FILE_NAME,"%s","<033>&a420h7150V")
            Writefile(L_FILE_NAME,"%s",l_copyright_6)
            Writefile(L_FILE_NAME,"%s"," ")
            Writefile(L_FILE_NAME,"%s",L_ARIAL_9)
            Writefile(L_FILE_NAME,"%s",sfsoptend:COPYRIGHT)
            Writefile(L_FILE_NAME,"%s","<033>&a200h+120V")

            L_STRING       = trun(sfscompany:name[1])
            L_FONT         =  trun(L_ARIAL_BOLD_9)
            L_PITCH_HALF   = 6
            L_PITCH_FULL   = 12
            L_VERTICAL_POS = "0"
            Include "center2.pro"

            Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
            Writefile(L_FILE_NAME,"%s","<033>&a4600h+20V")
            Writefile(L_FILE_NAME,"%s",Trun(SFSOPTEND:FORM_PRINT))
            Writefile(L_FILE_NAME,"%s"," ")
            Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD_9)
            Writefile(L_FILE_NAME,"%s",SFSOPTEND:EDITION_PRINT)
            Writefile(L_FILE_NAME,"%s","<033>&a200h+120V")

            Writefile(L_FILE_NAME,"%s%s%s%s%s%s",L_ARIAL_BOLD_9,"<033>&a180h+0V",
                                  "DECLARATIONS SUPPLEMENT - ",SFSSTATE:DESCRIPTION,
                                  "<033>&a140h+60V<033>*c5467h5v0P","<NL>")

            Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD_9)
            Writefile(L_FILE_NAME,"%s","<033>&a1950h-20V")
            Writefile(L_FILE_NAME,"%s","SPECIAL ENHANCEMENT ENDORSEMENT")
            Writefile(L_FILE_NAME,"%s","<033>&a140h+30V<033>*c5467h5v0P")

            Writefile(L_FILE_NAME,"%s%s%s",L_ARIAL_BOLD_9,"<033>&a180h+180V","Policy No: ")

            Writefile(L_FILE_NAME,"%s",Trun(SFSLINE:ALPHA))

            Writefile(L_FILE_NAME,"%n%s%s%d%s",SFPPRINT1:POLICY_NO,"<033>&a3250h+0V",
                                  "Effective: ",SFPNAME:EFF_DATE,"<NL>")

            Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD_9)
            Writefile(L_FILE_NAME,"%s","<033>&a600h+20V")
            Writefile(L_FILE_NAME,"%s","Each basic limit ")
            Writefile(L_FILE_NAME,"%s","<033>&a1310h-30V<033>*c90h2v0P")
            Writefile(L_FILE_NAME,"%s","<033>&a1400h+30V")
            Writefile(L_FILE_NAME,"%s"," or if increased, each revised limit ")
            Writefile(L_FILE_NAME,"%s","<033>&a2880h-30V<033>*c90h2v0P")
            Writefile(L_FILE_NAME,"%s","<033>&a3000h+30V")
            Writefile(L_FILE_NAME,"%s","shown below is")
            Writefile(L_FILE_NAME,"%s",L_ARIAL_ITALIC_BOLD_9)
            Writefile(L_FILE_NAME,"%s", " our ")
            Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD_9)
            Writefile(L_FILE_NAME,"%s","maximum limit of liability for the coverage ")
            Writefile(L_FILE_NAME,"%s","<NL>")

            Writefile(L_FILE_NAME,"%s%s%s%s",L_ARIAL_BOLD_9,"<033>&a600h+0V",
                                "in any occurrence, except as otherwise stated.","<NL>")

            Writefile(L_FILE_NAME,"%s",L_ARIAL_Italic_BOLD_9)
            Writefile(L_FILE_NAME,"%s","<033>&a180h+40V")
            Writefile(L_FILE_NAME,"%s","Your ")
            Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD_9)
            Writefile(L_FILE_NAME,"%s","Business / Operation:  ")
            Writefile(L_FILE_NAME,"%s",BOPGENERAL:BUSINESS_DESC[1])
            Writefile(L_FILE_NAME,"%s","<NL>")

            Writefile(L_FILE_NAME,"%s%s%s%s%n%s%s%s%n",L_ARIAL_BOLD,"<033>&a180h+0V",
                                "Location","<033>&a900h+0V",SFPLOCATION:PREM_NO,
                                "<033>&a1200h+0V","Building","<033>&a1900h+0V",
                                SFPLOCATION:BUILD_NO)

            Writefile(L_FILE_NAME,"%s%s%s%s",L_ARIAL_BOLD,"<033>&a900h+260V",
                                  "COVERAGE:   PART I","<NL>")
            L_COVERGE_POS = 180
            --Writefile(L_FILE_NAME,"%s","<033>&a140h-60V<033>*c5455h20v0P")
            L_BASIC_POS = 2550
            L_REVISED_POS = 2430
            L_DOLLAR_POS = 3575
            L_DEDUCTIBLE_POS = 1840
            L_FACTOR_POS = 2745
            L_REVISED_FACTOR_POS = 2350
            L_LIMIT_POS = 3650

            Access BOPGENERAL ,
               Set BOPGENERAL:POLICY_NO    = SFPPRINT1:POLICY_NO,
                   BOPGENERAL:POL_YEAR     = SFPPRINT1:POL_YEAR,
                   BOPGENERAL:END_SEQUENCE = SFPPRINT1:END_SEQUENCE,
                   BOPGENERAL:PREM_NO      = SFPLOCATION:PREM_NO,
                   BOPGENERAL:BUILD_NO     = SFPLOCATION:BUILD_NO,Generic

            Writefile(L_FILE_NAME,"%s%s%s%s%s%s%s%s%s%n%s%s%s%s%n%s%s%s",L_ARIAL_BOLD,
                                  "<033>&a180h+60V","Accounts Receivable ",L_ARIAL_8,
                                  "<033>&a-30V","(1)","<033>&a+30V",L_ARIAL,
                                  "<033>&a",L_BASIC_POS,"h+0V","Basic Limit:",
                                  L_LETTER_GOTHIC_BOLD_12,"<033>&a",L_DOLLAR_POS,"h+0V",
                                  L_ARIAL_BOLD,"$") --Basic Limit Accounts Receivable

            Do Print_with_or_without_enhancement(bpsdefault:ar_limit,bopgeneral:ar_limit,l_ar_limit,bopgeneral:special_enhancement)


            Writefile(L_FILE_NAME,"%s%s%s%n%s%s","<NL>",L_ARIAL_BOLD,"<033>&a",
                                  L_COVERGE_POS,"h+100V","Building")

            Writefile(L_FILE_NAME,"%s%s%s%n%s%s%s%s","<NL>",L_ARIAL_BOLD,"<033>&a",
                                  L_COVERGE_POS,"h+0V","E. ",l_arial,"Supplies and Materials: Loss by Theft")

            Writefile(L_FILE_NAME,"%s%s%n%s%s%s%n%s%s%s",L_ARIAL,"<033>&a",
                                    L_BASIC_POS,"h+0V","Basic Limit:",
                                    "<033>&a",L_DOLLAR_POS,"h+0V",L_ARIAL_BOLD,"$")

            If BPSSUPPLEMENTAL:SUPPLIES_LIMIT > 0 Then
              {

                Writefile(L_FILE_NAME,"%s%s%n%s%s%s",L_LETTER_GOTHIC_BOLD_12,"<033>&a",
                                  L_LIMIT_POS,"h+0V",
                                  Str(BPSSUPPLEMENTAL:SUPPLIES_LIMIT,"ZZZ,ZZZ,ZZ9"),
                                  "<NL>")
              }

            Writefile(L_FILE_NAME,"%s%s%n%s%s",L_ARIAL_BOLD,"<033>&a",
                                  L_COVERGE_POS,"h+190V","Building Code / Law Coverage ")

            Writefile(L_FILE_NAME,"%s%n%s%s%s","<033>&a",L_FACTOR_POS,
                                  "h+0V",L_ARIAL,"Factor:")                  --factor

            L_BUILDING_CODE_PERCENT = BPSSUPPLEMENTAL:BUILDING_CODE_PERCENT Divide 100

            Writefile(L_FILE_NAME,"%s%n%s%s%s%s","<033>&a",L_limit_POS,"h+0V",
                                  L_LETTER_GOTHIC_BOLD_12,
                                  Str(L_BUILDING_CODE_PERCENT,"ZZZZ,ZZZ.ZZ"),"<NL>")

            Writefile(L_FILE_NAME,"%s%n%s%s%s","<033>&a",L_REVISED_FACTOR_POS,
                                  "h+0V",L_ARIAL,"Revised Factor:")                  --revised factor

            If BOPGENERAL:BUILDING_CODE_PERCENT > BPSSUPPLEMENTAL:BUILDING_CODE_PERCENT then --L_BUILDING_CODE_PERCENT then
              {
                Writefile(L_FILE_NAME,"%s%s%n%s%s",L_LETTER_GOTHIC_BOLD_12,"<033>&a",
                                                   L_LIMIT_POS,"h+0V",
                                                   Str(BOPGENERAL:BUILDING_CODE_PERCENT Divide 100,"ZZZZ,ZZZ.ZZ"))
              }

            Writefile(L_FILE_NAME,"%s%s%s%n%s%s%s","<NL>",L_ARIAL_ITALIC_8,"<033>&a",
                                  L_COVERGE_POS + 200 ,"h+90V",
                                  "*  Does Not Apply if Insured on Actual Cash Value Basis",
                                  "<NL>")

            Writefile(L_FILE_NAME,"%s%s%n%s%s%s",L_ARIAL_BOLD,"<033>&a",
                                   L_COVERGE_POS,"h+90V",
                                   "Building Glass",
                                   "<NL>")

            Writefile(L_FILE_NAME,"%s%s%n%s%s",L_ARIAL,"<033>&a",
                                  L_REVISED_POS-600,"h+0V","Basic Limit per Occurrence:")

            Writefile(L_FILE_NAME,"%s%n%s%s%s","<033>&a",
                                  L_DOLLAR_POS,"h+0V",L_ARIAL_BOLD,"$")

            Writefile(L_FILE_NAME,"%s%s%n%s%s%s",L_LETTER_GOTHIC_BOLD_12,"<033>&a",
                                  L_LIMIT_POS,"h+0V",Str(BPSSUPPLEMENTAL:BUILDING_GLASS,"ZZZ,ZZZ,ZZ9"),"<NL>")

            Writefile(L_FILE_NAME,"%s%s%n%s%s",L_ARIAL,"<033>&a",L_REVISED_POS-1025,"h+0V",
                                  "Basic Limit on Any One Pane or Unit:")

            Writefile(L_FILE_NAME,"%s%n%s%s%s","<033>&a",
                                  L_DOLLAR_POS,"h+0V",L_ARIAL_BOLD,"$")

            Writefile(L_FILE_NAME,"%s%s%n%s%s%s%s",L_LETTER_GOTHIC_BOLD_12, "<033>&a",
                                  L_LIMIT_POS,"h+0V",
                                  Str(BPSSUPPLEMENTAL:GLASS_PANE,"ZZZ,ZZZ,ZZ9"),"<NL>","<NL>")

            Writefile(L_FILE_NAME,"%s%s%n%s%s%s%s%s%s%s",L_ARIAL_BOLD,"<033>&a",
                                  L_COVERGE_POS,"h+0V",
                                  "Business Personal Property ",L_ARIAL_8,
                                  "<033>&a-30V","(1)","<033>&a+30V","<NL>")

            Writefile(L_FILE_NAME,"%s%s%s%s%s%s%n%s%s%s%s%n%s%s%s",L_ARIAL,
                                  "<033>&a180h+10V","Customers' Property ",L_ARIAL_8,
                                  L_ARIAL,"<033>&a",L_BASIC_POS,"h+0V","Basic Limit:",
                                  L_LETTER_GOTHIC_BOLD_12,"<033>&a",L_DOLLAR_POS,"h+0V",
                                  L_ARIAL_BOLD,"$")

            Do Print_with_or_without_enhancement(BPSDEFAULT:customer_prop_limit,
                                                 bopgeneral:customer_prop_limit,
                                                 l_customer_prop_limit,
