    Writefile(L_FILE_NAME,"%s%s%n%s%s%s",L_ARIAL_BOLD,"<033>&a",L_COVERGE_POS,"h+0V",
                          "Outdoor (Exterior) Building Glass","<NL>")

    Writefile(L_FILE_NAME,"%s%s%n%s%s",L_ARIAL,"<033>&a",L_BASIC_POS,"h+0V",
