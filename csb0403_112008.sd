 Unsigned Ascii Number L_REVISED_FACTOR_POS;
 Unsigned Ascii Number L_BUILDING_CODE_PERCENT[3]/decimals=2;
 Unsigned Ascii Number L_LIMIT_POS;
 Unsigned Ascii Number L_LINE_PCOUNTER;
