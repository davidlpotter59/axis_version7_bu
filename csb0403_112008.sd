Procedure Definition

Procedure PRINT_LIMIT

{
  If SFSLINE:OPTION = "P" Then        -- means it is chatham square
    {
      If SCIPSCONTROL:PRINT_REVISED_ON_SUPP = 1 Then
        {
          If (L_LIMIT >= L_BPSDEFAULT_LIMIT) Or
              L_LIMIT >= L_REVISED_LIMIT  Then
            {
              Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
              Writefile(L_FILE_NAME,"%s","<033>&a")
              Writefile(L_FILE_NAME,"%n",L_LIMIT_POS)
              Writefile(L_FILE_NAME,"%s","h+0V")
              Writefile(L_FILE_NAME,"%s",Str(L_LIMIT,"ZZZ,ZZZ,ZZ9"))
      }  --l_limit is greater then both bpssupplemental and revised
    Else
      {
        If (L_bpssupplemental_LIMIT = L_REVISED_LIMIT) Then
          {
            Writefile(L_FILE_NAME,"%s","<033>&a")
            Writefile(L_FILE_NAME,"%n",L_LIMIT_POS)
            Writefile(L_FILE_NAME,"%s","h+0V")
            Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
            Writefile(L_FILE_NAME,"%s",Str(L_bpssupplemental_LIMIT,"ZZZ,ZZZ,ZZ9"))

          }
        Else
          If (L_bpssupplemental_LIMIT > L_REVISED_LIMIT) Then
            {
              Writefile(L_FILE_NAME,"%s","<033>&a")
              Writefile(L_FILE_NAME,"%n",L_LIMIT_POS)
              Writefile(L_FILE_NAME,"%s","h+0V")
              Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
              Writefile(L_FILE_NAME,"%s",Str(L_bpssupplemental_LIMIT,"ZZZ,ZZZ,ZZ9"))
            }
      }
    Writefile(L_FILE_NAME,"%s","<NL>")
    Writefile(L_FILE_NAME,"%s",L_ARIAL)
    Writefile(L_FILE_NAME,"%s","<033>&a")
    Writefile(L_FILE_NAME,"%n",L_REVISED_POS)
    Writefile(L_FILE_NAME,"%s","h+0V")
    Writefile(L_FILE_NAME,"%s","Revised Limit:")
    Writefile(L_FILE_NAME,"%s","<033>&a")
    Writefile(L_FILE_NAME,"%n",L_DOLLAR_POS)
    Writefile(L_FILE_NAME,"%s","h+0V")
    Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
    Writefile(L_FILE_NAME,"%s","$")
    Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)

    If L_REVISED_LIMIT > L_bpssupplemental_LIMIT Then
      {
        Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
        Writefile(L_FILE_NAME,"%s","<033>&a")
        Writefile(L_FILE_NAME,"%n",L_LIMIT_POS)
        Writefile(L_FILE_NAME,"%s","h+0V")
        Writefile(L_FILE_NAME,"%s",Str(L_REVISED_LIMIT,"ZZZ,ZZZ,ZZ9"))
     }
     Writefile(L_FILE_NAME,"%s","<NL>")
  } --end of print revised on supp
Else
  {
    If (L_LIMIT >= L_bpssupplemental_LIMIT) And
       (L_LIMIT >= L_REVISED_LIMIT) Then
      {
        Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
        Writefile(L_FILE_NAME,"%s","<033>&a")
        Writefile(L_FILE_NAME,"%n",L_LIMIT_POS)
        Writefile(L_FILE_NAME,"%s","h+0V")
        Writefile(L_FILE_NAME,"%s",Str(L_LIMIT,"ZZZ,ZZZ,ZZ9")) --basic limit
      }
    Else
      If (L_LIMIT < L_bpssupplemental_LIMIT) And       --default value is printed
         (L_bpssupplemental_LIMIT >= L_REVISED_LIMIT) Then
        {
          Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
          Writefile(L_FILE_NAME,"%s","<033>&a")
          Writefile(L_FILE_NAME,"%n",L_LIMIT_POS)
          Writefile(L_FILE_NAME,"%s","h+0V")
          Writefile(L_FILE_NAME,"%s",Str(L_bpssupplemental_LIMIT,"ZZZ,ZZZ,ZZ9")) --basic limit
        }
      Else
        {
          Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
          Writefile(L_FILE_NAME,"%s","<033>&a")
          Writefile(L_FILE_NAME,"%n",L_LIMIT_POS)
          Writefile(L_FILE_NAME,"%s","h+0V")
          Writefile(L_FILE_NAME,"%s",Str(L_REVISED_LIMIT,"ZZZ,ZZZ,ZZ9"))  --revised limit
        }
      Writefile(L_FILE_NAME,"%s","<NL>") --moves "Applies only with coverage B" down two lines
  }

    }
  Else  -- if not chatham square do below for limit
    {
      If SCIPSCONTROL:PRINT_REVISED_ON_SUPP = 1 Then
        {
          If (L_bpssupplemental_LIMIT >= L_REVISED_LIMIT) Then
            {
              Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
              Writefile(L_FILE_NAME,"%s","<033>&a")
              Writefile(L_FILE_NAME,"%n",L_LIMIT_POS)
              Writefile(L_FILE_NAME,"%s","h+0V")
              Writefile(L_FILE_NAME,"%s",Str(L_bpssupplemental_LIMIT,"ZZZ,ZZZ,ZZ9"))
            }  --if l_bpssupplemental_limit < l_revised_limit  do not print basic limit
          Writefile(L_FILE_NAME,"%s","<NL>")
          Writefile(L_FILE_NAME,"%s",L_ARIAL)
          Writefile(L_FILE_NAME,"%s","<033>&a")
          Writefile(L_FILE_NAME,"%n",L_REVISED_POS)
          Writefile(L_FILE_NAME,"%s","h+0V")
          Writefile(L_FILE_NAME,"%s","Revised Limit:")
          Writefile(L_FILE_NAME,"%s","<033>&a")
          Writefile(L_FILE_NAME,"%n",L_DOLLAR_POS)
          Writefile(L_FILE_NAME,"%s","h+0V")
          Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
          Writefile(L_FILE_NAME,"%s","$")
          Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
          If (L_bpssupplemental_LIMIT = L_REVISED_LIMIT) Then
            {
              Writefile(L_FILE_NAME,"%s","<033>&a")
              Writefile(L_FILE_NAME,"%n",L_LIMIT_POS)
              Writefile(L_FILE_NAME,"%s","h+0V")
              Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
              Writefile(L_FILE_NAME,"%s",Str(0,"ZZZ,ZZZ,ZZ9"))
              Writefile(L_FILE_NAME,"%s","<NL>")
            }
          Else
            {
              Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
              Writefile(L_FILE_NAME,"%s","<033>&a")
              Writefile(L_FILE_NAME,"%n",L_LIMIT_POS)
              Writefile(L_FILE_NAME,"%s","h+0V")
              Writefile(L_FILE_NAME,"%s",Str(L_REVISED_LIMIT,"ZZZ,ZZZ,ZZ9"))
              Writefile(L_FILE_NAME,"%s","<NL>")
           }
        }
      Else --do not print the revised limit
        {
          If (L_bpssupplemental_LIMIT >= L_REVISED_LIMIT) Then
            {
              Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
              Writefile(L_FILE_NAME,"%s","<033>&a")
              Writefile(L_FILE_NAME,"%n",L_LIMIT_POS)
              Writefile(L_FILE_NAME,"%s","h+0V")
              Writefile(L_FILE_NAME,"%s",Str(L_bpssupplemental_LIMIT,"ZZZ,ZZZ,ZZ9"))
              Writefile(L_FILE_NAME,"%s","<NL>")
            }  --print l_bpssupplemental_limit
          Else --print l_revised_limit
            {
              Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
              Writefile(L_FILE_NAME,"%s","<033>&a")
              Writefile(L_FILE_NAME,"%n",L_LIMIT_POS)
              Writefile(L_FILE_NAME,"%s","h+0V")
              Writefile(L_FILE_NAME,"%s",Str(L_REVISED_LIMIT,"ZZZ,ZZZ,ZZ9"))
              Writefile(L_FILE_NAME,"%s","<NL>")
            }
        }
    }
}

End
