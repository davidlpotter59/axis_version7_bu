      Writefile(L_FILE_NAME,"%s"," ")
      Writefile(L_FILE_NAME,"%s",L_SFPLOCATION_ZIP)
      Writefile(L_FILE_NAME,"%s","<NL>")

      Writefile(L_FILE_NAME,"%s","<033>&a140h-80V<033>*c5467h8v0P")

      L_STRING = "COVERAGE: PART I"
      L_FONT = L_ARIAL_bold
      L_PITCH_HALF = 5
      L_PITCH_FULL = 10
      L_VERTICAL_POS = "+160"
      L_HORIZONTAL_POS = "2880"
      Include "center3.pro"

      L_COVERGE_POS = 180
      --Writefile(L_FILE_NAME,"%s","<033>&a140h-60V<033>*c5455h20v0P")
      L_BASIC_POS = 2550
      L_REVISED_POS = 2430
      L_DOLLAR_POS = 3575
      L_DEDUCTIBLE_POS = 1840
      L_FACTOR_POS = 2745
      L_REVISED_FACTOR_POS = 2350
      L_LIMIT_POS = 3650

      Access BOPGENERAL ,
         Set BOPGENERAL:POLICY_NO    = SFPPRINT1:POLICY_NO,
             BOPGENERAL:POL_YEAR     = SFPPRINT1:POL_YEAR,
             BOPGENERAL:END_SEQUENCE = SFPPRINT1:END_SEQUENCE,
             BOPGENERAL:PREM_NO      = SFPLOCATION:PREM_NO,
             BOPGENERAL:BUILD_NO     = SFPLOCATION:BUILD_NO,Generic

      Access BPSSUPPLEMENTAL,
         Set BPSSUPPLEMENTAL:COMPANY_ID       = L_COMPANY_ID,
             BPSSUPPLEMENTAL:STATE            = L_STATE,
             BPSSUPPLEMENTAL:LINE_OF_BUSINESS = L_LINE_OF_BUSINESS, generic

       While BPSSUPPLEMENTAL:COMPANY_ID       = L_COMPANY_ID and
             BPSSUPPLEMENTAL:STATE            = L_STATE and
             BPSSUPPLEMENTAL:LINE_OF_BUSINESS = L_LINE_OF_BUSINESS
         {
           if BPSSUPPLEMENTAL:eff_date <= sfpname:eff_date then
             {
               l_eff_date = BPSSUPPLEMENTAL:eff_date
             }
           Next BPSSUPPLEMENTAL
         }

      Access BPSSUPPLEMENTAL,
         Set BPSSUPPLEMENTAL:COMPANY_ID       = SFPNAME:COMPANY_ID,
             BPSSUPPLEMENTAL:STATE            = L_STATE,
             BPSSUPPLEMENTAL:LINE_OF_BUSINESS = SFPNAME:LINE_OF_BUSINESS,
             BPSSUPPLEMENTAL:eff_date         = l_eff_date, exact

      Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
      Writefile(L_FILE_NAME,"%s","<033>&a180h+240V")
      Writefile(L_FILE_NAME,"%s","Accounts Receivable ")
      Writefile(L_FILE_NAME,"%s",L_ARIAL_8)
      Writefile(L_FILE_NAME,"%s","<033>&a-30V")
      Writefile(L_FILE_NAME,"%s","(1)")
      Writefile(L_FILE_NAME,"%s","<033>&a+30V")
      Writefile(L_FILE_NAME,"%s",L_ARIAL)
      Writefile(L_FILE_NAME,"%s","<033>&a")
      Writefile(L_FILE_NAME,"%n",L_BASIC_POS)
      Writefile(L_FILE_NAME,"%s","h+0V")
      Writefile(L_FILE_NAME,"%s","Basic Limit:")
      Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
      Writefile(L_FILE_NAME,"%s","<033>&a")
      Writefile(L_FILE_NAME,"%n",L_DOLLAR_POS)
      Writefile(L_FILE_NAME,"%s","h+0V")
      Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
      Writefile(L_FILE_NAME,"%s","$") --Basic Limit Accounts Receivable

      L_LIMIT                 = BPSSUPPLEMENTAL:AR_LIMIT  --(BPSDEFAULT:AR_PERCENT Divide 100)
      L_REVISED_LIMIT         = BOPGENERAL:AR_LIMIT
      L_BPSSUPPLEMENTAL_LIMIT = BPSSUPPLEMENTAL:AR_LIMIT --L_BPSDEFAULT_LIMIT = BPSDEFAULT:AR_LIMIT

      Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
      Writefile(L_FILE_NAME,"%s","<033>&a")
      Writefile(L_FILE_NAME,"%n",L_LIMIT_POS)
      Writefile(L_FILE_NAME,"%s","h+0V")
      Writefile(L_FILE_NAME,"%s",Str(BPSSUPPLEMENTAL:AR_LIMIT,"ZZZ,ZZZ,ZZ9"))

      Writefile(L_FILE_NAME,"%s",L_arial)
      Writefile(L_FILE_NAME,"%s","<033>&a")
      Writefile(L_FILE_NAME,"%n",L_LIMIT_POS+700)
      Writefile(L_FILE_NAME,"%s","h+0V")
      Writefile(L_FILE_NAME,"%s","or 5% of contents limit,")

      Writefile(L_FILE_NAME,"%s",L_arial)
      Writefile(L_FILE_NAME,"%s","<033>&a")
      Writefile(L_FILE_NAME,"%n",L_LIMIT_POS+700)
      Writefile(L_FILE_NAME,"%s","h+120V")
      Writefile(L_FILE_NAME,"%s","whichever is greater")

      Writefile(L_FILE_NAME,"%s","<NL>")
      Writefile(L_FILE_NAME,"%s",L_ARIAL)
      Writefile(L_FILE_NAME,"%s","<033>&a")
      Writefile(L_FILE_NAME,"%n",L_REVISED_POS)
      Writefile(L_FILE_NAME,"%s","h+0V")
      Writefile(L_FILE_NAME,"%s","Revised Limit:")
      Writefile(L_FILE_NAME,"%s","<033>&a")
      Writefile(L_FILE_NAME,"%n",L_DOLLAR_POS)
      Writefile(L_FILE_NAME,"%s","h+0V")
      Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
      Writefile(L_FILE_NAME,"%s","$")
      Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)

      If bopgeneral:ar_LIMIT > bpssupplemental:ar_LIMIT then
        {
          Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
          Writefile(L_FILE_NAME,"%s","<033>&a")
          Writefile(L_FILE_NAME,"%n",L_LIMIT_POS)
          Writefile(L_FILE_NAME,"%s","h+0V")
          Writefile(L_FILE_NAME,"%s",Str(bopgeneral:ar_LIMIT,"ZZZ,ZZZ,ZZ9"))
        }

--      Do PRINT_LIMIT

      Writefile(L_FILE_NAME,"%s","<NL>")
      Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
      Writefile(L_FILE_NAME,"%s","<033>&a")
      Writefile(L_FILE_NAME,"%n",L_COVERGE_POS)
      Writefile(L_FILE_NAME,"%s","h+100V")
      Writefile(L_FILE_NAME,"%s","Building")

      Writefile(L_FILE_NAME,"%s","<NL>")
      Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
      Writefile(L_FILE_NAME,"%s","<033>&a")
      Writefile(L_FILE_NAME,"%n",L_COVERGE_POS)
      Writefile(L_FILE_NAME,"%s","h+0V")
      Writefile(L_FILE_NAME,"%s","E. ")
      Writefile(L_FILE_NAME,"%s",l_arial)
      Writefile(L_FILE_NAME,"%s","Supplies and Materials Loss by Theft")

      Writefile(L_FILE_NAME,"%s",L_ARIAL)
      Writefile(L_FILE_NAME,"%s","<033>&a")
      Writefile(L_FILE_NAME,"%n",L_BASIC_POS)
      Writefile(L_FILE_NAME,"%s","h+0V")
      Writefile(L_FILE_NAME,"%s","Basic Limit:")
      Writefile(L_FILE_NAME,"%s","<033>&a")
      Writefile(L_FILE_NAME,"%n",L_DOLLAR_POS)
      Writefile(L_FILE_NAME,"%s","h+0V")
      Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD)
      Writefile(L_FILE_NAME,"%s","$")

      Writefile(L_FILE_NAME,"%s",L_LETTER_GOTHIC_BOLD_12)
      Writefile(L_FILE_NAME,"%s","<033>&a")
      Writefile(L_FILE_NAME,"%n",L_LIMIT_POS)
      Writefile(L_FILE_NAME,"%s","h+0V")
      Writefile(L_FILE_NAME,"%s",Str(BPSSUPPLEMENTAL:SUPPLIES_LIMIT,"ZZZ,ZZZ,ZZ9"))
